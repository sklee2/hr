<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" "http://castor.org/mapping.dtd">
<mapping>
    <description>
		Spring OXM and Castor
    </description>
    <class name="com.hyulib.login.domain.PortalLogin">
        <map-to xml="response" />
        <field name="data" type="com.hyulib.login.domain.Data">
            <bind-xml name="data" />
        </field>
        <field name="code" type="string">
            <bind-xml name="code" node="element"/>
        </field>
        <field name="msg" type="string">
            <bind-xml name="msg" node="element"/>
        </field>
        <field name="url" type="string">
            <bind-xml name="url" node="element"/>
        </field>
    </class>
    <class name="com.hyulib.login.domain.Data">
        <map-to xml="data" />
        <field name="name" type="string">
            <bind-xml name="name" node="element"/>
        </field>
        <field name="gaeinNo" type="string">
            <bind-xml name="gaeinNo" node="element"/>
        </field>
        <field name="userId" type="string">
            <bind-xml name="userId" node="element"/>
        </field>
        <field name="uuid" type="string">
            <bind-xml name="uuid" node="element"/>
        </field>
    </class>
 </mapping>  